{
  "name": "ossmconsole",
  "version": "2.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kiali/openshift-servicemesh-plugin.git"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "yarn clean && sh -ac '. ./.env.production; yarn ts-node node_modules/.bin/webpack'",
    "build:dev": "yarn clean && sh -ac '. ./.env.development; yarn ts-node node_modules/.bin/webpack'",
    "start": "sh -ac '. ./.env.development; yarn ts-node node_modules/.bin/webpack serve'",
    "start-console": "./start-console.sh",
    "cypress": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:junit": "cypress run --reporter cypress-multi-reporters --reporter-options configFile=cypress/reporter-config.json",
    "cypress:delete:reports": "rm cypress/results/* || true",
    "cypress:combine:reports": "jrm cypress/results/combined-report.xml \"cypress/results/*.xml\"",
    "i18n": "yarn i18n:kiali && yarn i18n:openshift",
    "i18n:kiali": "i18next 'src/kiali/**/*.{ts,tsx}' -o 'src/kiali/locales/$LOCALE/$NAMESPACE.json' -c './i18next-parser.config.ts' --silent",
    "i18n:openshift": "i18next 'src/openshift/**/*.{ts,tsx}' -o 'src/openshift/locales/$LOCALE/$NAMESPACE.json' -c './i18next-parser.config.ts' --silent",
    "ts-node": "yarn i18n && ts-node -O '{\"module\":\"commonjs\"}'",
    "lint": "eslint --ext ts,tsx src/openshift",
    "lint:precommit": "if git diff --name-only HEAD | grep -E '\\.tsx?$'; then npm run lint; else true; fi",
    "prettier": "prettier --write \"**/*.{ts,tsx,scss,json}\"",
    "prepare": "cd .. && husky install plugin/.husky",
    "pre-commit": "yarn run pretty-quick --staged --no-restage --bail --pattern ['**/*.{ts,tsx,scss,json}', '!src/kiali'] && npm run lint:precommit"
  },
  "dependencies": {
    "@openshift-console/dynamic-plugin-sdk": "1.0.0",
    "@patternfly/patternfly": "^5.4.0",
    "@patternfly/react-charts": "^7.4.0",
    "@patternfly/react-core": "^5.4.0",
    "@patternfly/react-icons": "^5.4.0",
    "@patternfly/react-table": "^5.4.0",
    "@patternfly/react-topology": "^5.4.0",
    "axios": "^1.7.4",
    "bootstrap-slider-without-jquery": "10.0.0",
    "cy-node-html-label": "2.0.0",
    "cytoscape": "3.15.5",
    "cytoscape-canvas": "3.0.1",
    "cytoscape-popper": "1.0.7",
    "dagre": "0.8.5",
    "deep-freeze": "0.0.1",
    "eventemitter3": "4.0.7",
    "i18next": "^23.10.0",
    "i18next-http-backend": "^2.5.0",
    "js-yaml": "^4.1.0",
    "lodash-es": "^4.17.21",
    "micro-memoize": "4.0.9",
    "moment": "^2.29.4",
    "react": "^17.0.1",
    "react-ace": "9.1.3",
    "react-copy-to-clipboard": "5.x",
    "react-datepicker": "3.8.0",
    "react-dom": "^17.0.1",
    "react-flexview": "4.0.3",
    "react-i18next": "^11.7.3",
    "react-redux": "7.2.2",
    "react-resize-detector": "3.4.0",
    "react-router": "5.3.x",
    "react-router-dom": "5.3.x",
    "react-router-dom-v5-compat": "^6.11.2",
    "react-virtualized": "9.x",
