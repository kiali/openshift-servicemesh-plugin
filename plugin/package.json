{
  "name": "ossmconsole",
  "version": "0.4.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kiali/kiali.git"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "yarn clean && sh -ac '. ./.env.production; yarn ts-node node_modules/.bin/webpack'",
    "build:dev": "yarn clean && sh -ac '. ./.env.development; yarn ts-node node_modules/.bin/webpack'",
    "start": "sh -ac '. ./.env.development; yarn ts-node node_modules/.bin/webpack serve'",
    "start-console": "./start-console.sh",
    "i18n": "i18next \"src/**/*.{js,jsx,ts,tsx}\" [-oc] -c i18next-parser.config.js",
    "ts-node": "ts-node -O '{\"module\":\"commonjs\"}'",
    "lint": "eslint --ext js,ts,tsx src",
    "lint:precommit": "if git diff --name-only HEAD | grep -E '\\.tsx?$'; then npm run lint; else true; fi",
    "lint:fix": "eslint --ext js,ts,tsx --fix src",
    "prettier": "prettier --write \"{src/**/*.{js,jsx,ts,tsx,json,yml,css,scss},travis.yml,*.json}\"",
    "prepare": "cd .. && husky install plugin/.husky",
    "pre-commit": "yarn run pretty-quick --staged --pattern \"{src/**/*.{js,jsx,ts,tsx,json,yml,css,scss},travis.yml,*.json}\" && npm run lint:precommit"
  },
  "dependencies": {
    "@kiali/core-ui": "0.15.0",
    "@kiali/types": "0.15.0",
    "@openshift-console/dynamic-plugin-sdk": "0.0.13",
    "@patternfly/quickstarts": "2.0.1",
    "@patternfly/react-core": "4.206.2",
    "@patternfly/react-table": "^4.90.3",
    "axios": "0.21.4",
    "bootstrap-slider-without-jquery": "10.0.0",
    "comment-json": "^4.1.1",
    "i18next": "^19.8.3",
    "react": "17.0.2",
    "react-copy-to-clipboard": "5.x",
    "react-datepicker": "3.8.0",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.7.3",
    "react-redux": "7.2.2",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "redux": "4.0.3",
    "redux-persist": "5.10.0",
    "regression": "^2.0.1",
    "typestyle": "2.4.0",
    "victory": "36.3.0"
  },
  "devDependencies": {
    "@openshift-console/dynamic-plugin-sdk-internal": "0.0.11",
    "@openshift-console/dynamic-plugin-sdk-webpack": "0.0.8",
    "@openshift-console/plugin-shared": "^0.0.1",
    "@types/node": "^16.11.6",
    "@types/react": "^17.0.34",
    "@types/react-helmet": "^6.1.4",
    "@types/react-redux": "7.1.7",
    "@types/react-router-dom": "^5.3.2",
    "@types/webpack-dev-server": "^4.5.0",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "css-loader": "^6.5.1",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.3",
    "sass": "^1.62.1",
    "sass-loader": "^13.3.0",
    "showdown": "1.8.6",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.5.0"
  },
  "resolutions": {
    "@types/react": "17.0.53"
  },
  "consolePlugin": {
    "name": "ossmconsole",
    "version": "0.1.0",
    "displayName": "OpenShift Service Mesh Console",
    "description": "Provides Service Mesh/Istio Observability",
    "exposedModules": {
      "reduxReducer": "./utils/reducer.ts",
      "OverviewPage": "./openshift/OverviewPage",
      "GraphPage": "./openshift/GraphPage",
      "IstioConfigList": "./openshift/IstioConfigList",
      "MeshTab": "./openshift/MeshTab/MeshTab",
      "IstioMeshTab": "./openshift/MeshTab/IstioMesh"
    },
    "dependencies": {
      "@console/pluginAPI": "*"
    }
  }
}
