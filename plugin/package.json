{
  "name": "ossmconsole",
  "version": "1.73.23",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kiali/kiali.git"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "yarn clean && sh -ac '. ./.env.production; yarn ts-node node_modules/.bin/webpack'",
    "build:dev": "yarn clean && sh -ac '. ./.env.development; yarn ts-node node_modules/.bin/webpack'",
    "start": "sh -ac '. ./.env.development; yarn ts-node node_modules/.bin/webpack serve'",
    "start-console": "./start-console.sh",
    "cypress": "cypress open",
    "cypress:run": "cypress run $REPORTER -e TAGS=\"@install\" && cypress run $REPORTER -e TAGS=\"not @install and not @uninstall\" && cypress run $REPORTER -e TAGS=\"@uninstall\"",
    "cypress:run:junit": "REPORTER='--reporter cypress-multi-reporters --reporter-options configFile=cypress/reporter-config.json' npm run cypress:run ",
    "cypress:delete:reports": "rm cypress/results/* || true",
    "cypress:combine:reports": "jrm cypress/results/combined-report.xml \"cypress/results/*.xml\"",
    "i18n": "i18next \"src/**/*.{js,jsx,ts,tsx}\" [-oc] -c i18next-parser.config.js",
    "ts-node": "ts-node -O '{\"module\":\"commonjs\"}'",
    "lint": "eslint --ext js,ts,tsx src",
    "lint:precommit": "if git diff --name-only HEAD | grep -E '\\.tsx?$'; then npm run lint; else true; fi",
    "lint:fix": "eslint --ext js,ts,tsx --fix src",
    "prettier": "prettier --write \"**/*.{ts,tsx,scss,json}\"",
    "prepare": "cd .. && husky install plugin/.husky",
    "pre-commit": "yarn run pretty-quick --staged --no-restage --bail --pattern \"**/*.{ts,tsx,scss,json}\" && npm run lint:precommit"
  },
  "dependencies": {
    "@openshift-console/dynamic-plugin-sdk": "0.0.18",
    "@patternfly/patternfly": "4.224.2",
    "@patternfly/react-charts": "6.94.18",
    "@patternfly/react-core": "4.276.6",
    "@patternfly/react-table": "4.112.39",
    "@patternfly/react-topology": "4.91.27",
    "axios": "^1.7.4",
    "bootstrap-slider-without-jquery": "10.0.0",
    "cy-node-html-label": "2.0.0",
    "cytoscape": "3.15.5",
    "cytoscape-canvas": "3.0.1",
    "cytoscape-popper": "1.0.7",
    "dagre": "0.8.2",
    "deep-freeze": "0.0.1",
    "eventemitter3": "4.0.7",
    "js-yaml": "^3.13.1",
    "json-beautify": "1.0.1",
    "lodash-es": "^4.17.21",
    "micro-memoize": "4.0.9",
    "moment": "^2.29.4",
    "react": "^17.0.1",
    "react-ace": "9.1.3",
    "react-copy-to-clipboard": "5.x",
    "react-datepicker": "3.8.0",
    "react-dom": "^17.0.1",
    "react-flexview": "4.0.3",
    "react-redux": "7.2.2",
    "react-resize-detector": "3.4.0",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "react-virtualized": "9.x",
    "redux": "4.0.1",
    "redux-persist": "5.10.0",
    "redux-persist-transform-filter": "^0.0.22",
    "redux-thunk": "2.4.0",
    "regression": "^2.0.1",
    "reselect": "4.0.0",
    "screenfull": "5.0.2",
    "tippy.js": "3.4.1",
    "typesafe-actions": "^4.2.1",
    "typestyle": "^2.4.0",
    "victory-box-plot": "^36.6.7",
    "visibilityjs": "2.0.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@badeball/cypress-cucumber-preprocessor": "^20.1.2",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.0",
    "@openshift-console/dynamic-plugin-sdk-webpack": "0.0.9",
    "@types/cytoscape": "3.14.0",
    "@types/enzyme": "3.10.5",
    "@types/jest": "23.3.10",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash-es": "4.17.x",
    "@types/node": "^18.17.14",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.1",
    "@types/react-redux": "7.1.7",
    "@types/react-router-dom": "^5.2.0",
